{
  "name": "moamalat",
  "version": "1.0.3",
  "description": "Moamalat server sdk",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moneszarrugh/moamalat-node.git"
  },
  "keywords": [
    "moamalat",
    "payment",
    "gateway",
    "sdk",
    "libya"
  ],
  "author": "Mones Zarrugh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moneszarrugh/moamalat-node/issues"
  },
  "homepage": "https://github.com/moneszarrugh/moamalat-node#readme",
  "devDependencies": {
    "@types/node": "^18.7.6",
    "@types/node-fetch": "^2.6.2",
    "tsup": "^6.2.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "dayjs": "^1.11.5",
    "node-fetch": "^2.6.7"
  }
}
